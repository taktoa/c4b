{ T }
assert y >= 0;
loop
  (
{ -y ≤ 0 }
    if x - y <= 0
{ x - y ≤ 0 /\ -y ≤ 0 }
      break;
{ -y ≤ 0 /\ -x + y + 1 ≤ 0 }
    x -= 1;
{ -y ≤ 0 /\ -x + y ≤ 0 }
    x -= y;
{ -y ≤ 0 /\ -x ≤ 0 }
    z = y;
    loop
      (
{ -x ≤ 0 /\ -y ≤ 0 /\ -y + z ≤ 0 }
        if z <= 0
{ z ≤ 0 /\ -x ≤ 0 /\ -y ≤ 0 /\ -y + z ≤ 0 }
          break;
        z -= 1
{ -y + z + 1 ≤ 0 /\ -y ≤ 0 /\ -x ≤ 0 /\ -z ≤ 0 }
      )
{ -y ≤ 0 /\ -x ≤ 0 /\ z ≤ 0 }
  )
{ -y ≤ 0 /\ x - y ≤ 0 }
